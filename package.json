{
  "name": "valstro-cli-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": "jest --verbose --forceExit",
    "watch-ts": "tsc -w",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write '**/*.ts'",
    "format:check": "prettier --list-different --loglevel=debug '**/*.ts'",
    "prepare": "cd .. && husky install matching_service/.husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sam-codes-1/valstro-cli-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sam-codes-1/valstro-cli-app.git"
  },
  "homepage": "https://github.com/sam-codes-1/valstro-cli-app.git#readme",
  "devDependencies": {
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "socket.io-client": "^4.5.1"
  }
}
